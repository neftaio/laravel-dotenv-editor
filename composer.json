{
    "name": "neftaio/laravel-dotenv-editor",
    "description": "A fork package for editing the .env file in your Laravel root, created initially for Fabian Hagen <fabianhagen87@gmail.com>.",
    "keywords": ["laravel", "dotenv", "dotenv-editor"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Neftali Forero",
        "email": "neftaio@gmail.com"
    }],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Neftaio\\DotenvEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neftaio\\DotenvEditor\\DotenvEditorServiceProvider"
            ],
            "aliases": {
                "DotenvEditor": "Neftaio\\DotenvEditor\\DotenvEditorFacade"
            }
        }
    }
}